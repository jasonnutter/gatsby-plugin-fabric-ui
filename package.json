{
    "name": "gatsby-plugin-fabric-ui",
    "version": "1.0.4",
    "description": "Gatsby plugin for the Fabric UI framework",
    "main": "gatsby-ssr.js",
    "scripts": {
        "format": "prettier --write gatsby-ssr.jsx",
        "build": "babel gatsby-ssr.jsx --out-file gatsby-ssr.js",
        "prepublish": "npm run build",
        "test": "npm run build && cd test && npm run build"
    },
    "files": [
        "gatsby-ssr.js"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/jasonnutter/gatsby-plugin-fabric-ui.git"
    },
    "keywords": [
        "gatsby",
        "gatsby-plugin",
        "fabric"
    ],
    "author": "Jason Nutter <jasondnutter@gmail.com>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/jasonnutter/gatsby-plugin-fabric-ui/issues"
    },
    "homepage": "https://github.com/jasonnutter/gatsby-plugin-fabric-ui#readme",
    "dependencies": {
        "@microsoft/load-themed-styles": "^1.8.33"
    },
    "devDependencies": {
        "@babel/cli": "^7.1.2",
        "@babel/core": "^7.1.2",
        "@babel/preset-env": "^7.1.0",
        "@babel/preset-react": "^7.0.0",
        "prettier": "^1.14.3",
        "react": "^16.0.0",
        "react-dom": "^16.0.0"
    },
    "peerDependencies": {
        "react": "^16.0.0",
        "react-dom": "^16.0.0"
    }
